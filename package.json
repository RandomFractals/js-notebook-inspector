	],
	"license": "Apache-2.0",
	"readme": "README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/RandomFractals/js-notebook-inspector"
	},
	"bugs": "https://github.com/RandomFractals/js-notebook-inspector/issues",
	"homepage": "https://github.com/RandomFractals/js-notebook-inspector/README.md",
	"keywords": [
		"observable",
		"javascript",
		"notebook",
		"inspector 🕵️"
	],
	"galleryBanner": {
		"color": "#333",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.44.0",
		"node": ">=10.2.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:notebook-inspector.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "notebook-inspector.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
