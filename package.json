{
  "name": "js-notebook-inspector",
  "displayName": "Observable JS Notebook Inspector",
  "description": "Observable JS Notebook Inspector",
  "version": "1.0.0",
  "icon": "images/notebook.png",
  "publisher": "RandomFractalsInc",
  "author": "Taras Novak a.k.a. dataPixy devTools maker :)",
  "contributors": [
    "Taras Novak"
  ],
  "license": "Apache-2.0",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/RandomFractals/js-notebook-inspector"
  },
  "bugs": "https://github.com/RandomFractals/js-notebook-inspector/issues",
  "homepage": "https://github.com/RandomFractals/js-notebook-inspector/README.md",
  "keywords": [
    "observable",
    "javascript",
    "notebook",
    "inspector ðŸ•µï¸"
  ],
  "galleryBanner": {
    "color": "#333",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.44.0",
    "node": ">=10.2.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:js.notebook.url"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "js.notebook.url",
        "title": "View Notebook from URL",
        "category": "JS Notebook"
      }
    ],
    "keybindings": [
      {
        "command": "js.notebook.url",
        "key": "ctrl+alt+o"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.4",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@observablehq/inspector": "^3.2.1",
    "@observablehq/parser": "^4.0.1",
    "@observablehq/runtime": "^4.7.0",
    "superagent": "^5.2.2"
  }
